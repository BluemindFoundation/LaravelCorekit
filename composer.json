{
    "name": "bluemind/laravelcorekit",
    "description": "CoreKit provides a centralized set of helpers, response macros, and enums for shared use across Laravel applications.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "bluemind"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "nesbot/carbon": "^3.10",
        "symfony/http-foundation": "^7.2",
        "illuminate/database": "^12.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Corekit\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Corekit\\CorekitServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
